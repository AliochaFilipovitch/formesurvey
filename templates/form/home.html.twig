    <div class="cta-content">
      <div class="container">
        <h2>Créée par des agents, pour les agents de la Fonction Publique.</h2>
        <a href="{{ path('form') }}" class="btn btn-outline btn-xl js-scroll-trigger" target="_blank">Go ! <i class="fas fa-angle-right"></i></a>
      </div>
    </div>
    <div class="overlay"></div>
  </section>

